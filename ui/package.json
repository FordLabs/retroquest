{
  "name": "retroquest-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration local",
    "build": "ng build",
    "build-prod": "ng build --prod --build-optimizer --output-path=../api/src/main/resources/static",
    "test": "yarn unit-watch",
    "unit": "jest",
    "unit-watch": "jest --watchAll",
    "unit-coverage": "jest --coverage=true",
    "lint": "ng lint --cache",
    "lint-fix": "ng lint --fix",
    "sonarqube": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_UI_TOKEN",
    "postinstall": "ngcc",
    "prepare": "cd .. && husky install ui/.husky",
    "cypress": "cypress run --headless --browser chrome",
    "cypress-supervise": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~11.2.7",
    "@angular/service-worker": "~11.2.7",
    "@elderbyte/ts-logger": "^0.1.0",
    "@elderbyte/ts-stomp": "^2.1.2",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "ng-recaptcha": "^7.0.0",
    "ngx-infinite-scroll": "^7.1.0",
    "rxjs": "^6.5.4",
    "sonar-scanner": "^3.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@angular/language-service": "~11.2.7",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@testing-library/angular": "^10.5.0",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/user-event": "^13.1.1",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "~13.1.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "ajv": "^6.12.4",
    "cypress": "^6.8.0",
    "cypress-angular-unit-test": "^3.6.1",
    "cypress-jest-adapter": "^0.1.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "prettier": "^2.2.1",
    "puppeteer": "^1.5.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "ts-jest": "^26.5.4",
    "ts-mockito": "^2.3.1",
    "ts-node": "~8.5.0",
    "typescript": "~4.0.7",
    "webpack": "^4.29.6"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd ui && npx lint-staged"
    }
  }
}
