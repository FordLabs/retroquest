{
  "name": "retroquest-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration local",
    "build": "ng build",
    "build-prod": "ng build --configuration production --build-optimizer --output-path=../api/src/main/resources/static",
    "test": "yarn unit-watch",
    "unit": "jest",
    "unit-watch": "jest --watchAll",
    "unit-coverage": "jest --coverage=true",
    "lint": "ng lint --cache",
    "lint-fix": "ng lint --fix",
    "postinstall": "ngcc",
    "prepare": "cd .. && husky install ui/.husky",
    "cypress": "cypress run --headless --browser chrome",
    "cypress-supervise": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "~11.2.7",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~12.1.1",
    "@angular/service-worker": "~12.1.1",
    "@fontsource/carter-one": "^4.4.5",
    "@fontsource/open-sans": "^4.4.5",
    "@fontsource/quicksand": "^4.4.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "ng-recaptcha": "^8.0.0",
    "ngx-infinite-scroll": "^10.0.0",
    "rxjs": "^7.0.0",
    "sonar-scanner": "^3.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.0",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/language-service": "~12.1.1",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@testing-library/angular": "^10.5.0",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/user-event": "^13.1.1",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "~13.1.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "ajv": "^6.12.4",
    "cypress": "^6.8.0",
    "cypress-angular-unit-test": "^3.6.1",
    "cypress-jest-adapter": "^0.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "prettier": "^2.2.1",
    "puppeteer": "^1.5.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "ts-jest": "^26.5.4",
    "ts-mockito": "^2.3.1",
    "ts-node": "~8.5.0",
    "typescript": "~4.3.5",
    "webpack": "^4.29.6"
  },
  "lint-staged": {
    "*.{js,ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd ui && npx lint-staged"
    }
  }
}
