{
  "name": "retroquest-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration local",
    "start:react": "ng serve --configuration local,react",
    "build": "ng build",
    "prebuild-prod": "node getApplicationVersion.js",
    "build-prod": "ng build --configuration production --build-optimizer --output-path=../api/src/main/resources/static",
    "build-prod:react": "ng build --configuration production,react-prod --build-optimizer --output-path=../api/src/main/resources/static",
    "test": "yarn unit-watch",
    "unit": "jest",
    "unit-watch": "jest --watchAll --runInBand",
    "unit-coverage": "jest --coverage=true --runInBand",
    "lint": "ng lint --cache",
    "lint-fix": "ng lint --fix",
    "eslint": "eslint 'src/**/*.+(js|ts|tsx|test.tsx)' 'cypress/**/*.+(js|spec.js)'",
    "eslint:fix": "eslint --fix 'src/**/*.+(js|ts|tsx|test.tsx)' 'cypress/**/*.+(js|spec.js)'",
    "prettier": "prettier --check '**/*.{scss,tsx,ts,js}'",
    "prettier:fix": "prettier --write '**/*.{scss,tsx,ts,js}'",
    "postinstall": "ngcc",
    "prepare": "cd .. && husky install ui/.husky",
    "cypress": "cypress run --headless",
    "cypress-supervise": "cypress open",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@angular/service-worker": "~13.1.1",
    "@fontsource/carter-one": "^4.4.5",
    "@fontsource/open-sans": "^4.4.5",
    "@fontsource/quicksand": "^4.4.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "ng-recaptcha": "^8.0.0",
    "ngx-infinite-scroll": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.5.2",
    "rxjs": "^7.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@angular/language-service": "~13.1.1",
    "@babel/core": "^7.14.8",
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-docs": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.13",
    "@storybook/theming": "^6.4.13",
    "@testing-library/angular": "^11.0.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.1.1",
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "~13.1.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "ajv": "^6.12.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "cypress": "^9.2.0",
    "cypress-angular-unit-test": "^3.6.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-unused-imports": "^1.1.1",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^5.2.0",
    "jest": "^27.4.7",
    "jest-axe": "^5.0.1",
    "jest-preset-angular": "^11.0.1",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "prettier": "^2.2.1",
    "puppeteer": "^1.5.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.12.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-prettier": "^1.2.0",
    "ts-jest": "^27.1.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "~8.5.0",
    "typescript": "~4.5.4",
    "webpack": "^5.20.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd ui && npx lint-staged"
    }
  }
}
